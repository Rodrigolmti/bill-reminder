<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BRInputTextField">
        <attr name="inputLabel" format="string"/>
        <attr name="inputHint" format="string"/>
        <attr name="inputValue" format="string"/>
        <attr name="inputError" format="string"/>
        <attr name="inputType" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BRSpinnerField">
        <attr name="spinnerLabel" format="string" />
        <attr name="spinnerError" format="string" />
        <attr name="spinnerPrompt" format="string" />
